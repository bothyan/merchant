<view class="container">
    <view class="header">
        <view class="header-hd header-hd1">
            <image src="../../images/index/icon1.png" style="width:66rpx;height:66rpx;"></image>  
            <text>扫码收款</text>      
        </view>
        <view class="header-hd header-hd2">
            <image src="../../images/index/icon2.png" style="width:66rpx;height:74rpx;"></image>  
            <text>查看流水</text>      
        </view>
        <view class="header-hd header-hd3">
            <image src="../../images/index/icon3.png" style="width:80rpx;height:62rpx;"></image>  
            <text>发票管理</text>      
        </view>
    </view>
    <view class="nav">
        <view class="nav-hd">
            <image src="../../images/index/nav1.png" style="width:68rpx;height:43rpx;"></image>  
            <text>优惠券</text>  
        </view>
        <view class="nav-hd" bindtap="members">
            <image src="../../images/index/nav2.png" style="width:64rpx;height:48rpx;"></image>  
            <text>会员管理</text>  
        </view>
        <view class="nav-hd">

            <image src="../../images/index/titleb.png" style="width:11rpx;height:29rpx;"></image>  
            <text>今日数据</text>
        </view>
        <view class="datas">
            <view class="datas-hd datas-hd1">
                <text class="data-t">33200.9</text>

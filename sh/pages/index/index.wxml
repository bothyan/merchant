        </view>
        <view class="nav-hd">
            <image src="../../images/index/nav5.png" style="width:43rpx;height:52rpx;"></image>  
            <text>流水贷</text>  
        </view>
        <view class="nav-hd">
            <image src="../../images/index/nav6.png" style="width:50rpx;height:49rpx;"></image>  
            <text>扫码开票</text>  

            </view>
            <view class="datas-hd datas-hd2">
                <text class="data-t">245</text>
                <text class="data-num">今日笔数</text>
            </view>
            <view class="datas-hd datas-hd3">
                <text class="data-t">890.2</text>
                <text class="data-num">平均客单价</text>
            </view>
        </view>
    </view>
    <view class="model active">
        <view class="titlebox">
            <image src="../../images/index/titleb.png" style="width:11rpx;height:29rpx;"></image>  
            <text>营销活动</text>
        </view>
    </view>
